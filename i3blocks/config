[sink-volume]
command=i3widgets sink-volume
interval=once
signal=10
markup=pango

[memory]
command=i3widgets memory
interval=10
markup=pango

[battery]
command=i3widgets battery --device-path /org/freedesktop/UPower/devices/battery_BAT0
interval=5
markup=pango

[time]
command=i3widgets time-zh
interval=5
markup=pango
